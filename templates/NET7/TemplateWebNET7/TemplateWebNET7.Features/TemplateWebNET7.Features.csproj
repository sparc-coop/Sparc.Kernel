<Project Sdk="Microsoft.NET.Sdk.Web">

	<PropertyGroup>
		<TargetFramework>net7.0</TargetFramework>
		<Nullable>enable</Nullable>
		<ImplicitUsings>enable</ImplicitUsings>
		<Platforms>AnyCPU;x64</Platforms>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly.Server" Version="7.0.1" />
		<PackageReference Include="Sparc.Blossom.Server" Version="7.1.0-pre.13" />
		<!--#if (AddAzureADAuth)-->
		<PackageReference Include="Sparc.Blossom.Authentication.AzureADB2C" Version="7.1.0-pre.2" />
		<!--#endif-->
		<!--#if (AddCosmos)-->
		<PackageReference Include="Sparc.Blossom.Data.Cosmos" Version="7.1.0-pre.2" />
		<!--#endif-->
		<!--#if (AddSQL)-->
		<PackageReference Include="Sparc.Blossom.Data.SqlServer" Version="7.1.0-pre.1" />
		<!--#endif-->
		<!--#if (AddAzureNotifications)-->
		<PackageReference Include="Sparc.Blossom.Realtime.Azure" Version="7.1.0-pre.1" />
		<!--#endif-->
		<!--#if (AddTwilio)-->
		<PackageReference Include="Sparc.Blossom.Realtime.Twilio" Version="7.1.0-pre.1" />
		<!--#endif-->
		<!--#if (AddAzureStorage)-->
		<PackageReference Include="Sparc.Blossom.Data.AzureStorage" Version="7.1.0-pre.1" />
		<!--#endif-->
	</ItemGroup>




</Project>
